generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model instagram_session {
  id         Int     @id @default(autoincrement())
  sessionid  String  @db.VarChar(50)
  csrftoken  String  @db.VarChar(50)
  mid        String? @db.VarChar(50)
  ig_did     String? @db.VarChar(50)
  rur        String? @db.VarChar(50)
  ds_user_id String  @db.VarChar(50)
  id_user    Int
  users      users   @relation(fields: [id_user], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "instagram_session_ibfk_1")

  @@index([id_user], map: "id_user")
}

model users {
  id                Int                 @id @default(autoincrement())
  mail              String              @unique(map: "mail") @db.VarChar(155)
  clef_login        String?             @db.VarChar(50)
  sel               String?             @db.VarChar(50)
  instagram_session instagram_session[]
}
